    "@types/react-dom": "18",
    "autoprefixer": "10.4.16",
    "copyfiles": "^2.4.1",
    "eslint": "8",
    "eslint-config-next": "14.0.4",
    "nodemon": "3.0.2",
    "postcss": "8",
    "tailwindcss": "3.3.6",
    "ts-node": "10.9.2",
    "typescript": "5"
  }
